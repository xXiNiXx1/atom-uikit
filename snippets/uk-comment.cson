'.text.html':
  'Accordion':
    'prefix': 'uk-comment'
    'body': """
    <article class="uk-comment">
      <header class="uk-comment-header">
        <img class="uk-comment-avatar" src="$1" width="50" height="50" alt="">
        <h4 class="uk-comment-title">$2</h4>
        <div class="uk-comment-meta">$3</div>
      </header>
      <div class="uk-comment-body">
        <p>$4</p>
      </div>
    </article>
    """
